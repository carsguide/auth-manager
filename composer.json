{
    "name": "carsguide/auth-manager",
    "description": "Auth0 wrapper for Lumen",
    "keywords": [
        "Auth0",
        "Lumen auth0"
    ],
    "homepage": "https://github.com/carsguide/auth-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Anam Hossain",
            "email": "anam.hossain@carsguide.com.au"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "auth0/auth0-php": "7.7.0",
        "illuminate/http": "^9.0",
        "illuminate/cache": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/log": "^9.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/cache": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.5",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Carsguide\\Auth\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carsguide\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carsguide\\Auth\\Providers\\AuthManagerServiceProvider"
            ],
            "aliases": {
                "AuthManager": "Carsguide\\Auth\\Facades\\AuthManager"
            }
        }
    }
}
