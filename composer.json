{
    "name": "carsguide/auth-manager",
    "description": "Auth0 wrapper for Lumen",
    "keywords": [
        "Auth0",
        "Lumen auth0"
    ],
    "homepage": "https://github.com/carsguide/auth-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Anam Hossain",
            "email": "anam.hossain@carsguide.com.au"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "illuminate/support": "^8.0",
        "auth0/auth0-php": "^5.0||^8.0.0",
        "illuminate/http": "^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/log": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Carsguide\\Auth\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carsguide\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carsguide\\Auth\\Providers\\AuthManagerServiceProvider"
            ],
            "aliases": {
                "AuthManager": "Carsguide\\Auth\\Facades\\AuthManager"
            }
        }
    }
}
